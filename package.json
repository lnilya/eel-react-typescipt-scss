{
  "name": "eel-react-typescript-scss",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "sass": "^1.38.2",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start:eel": "python3 index.py --develop",
    "build:eel": "python3 -m eel index.py build --onefile --noconsole --name react-eel-app --icon icon.icns",
    "clean:build": "rm -rf build dist",
    "start": "npm-run-all -p start:*",
    "build": "npm-run-all clean:build build:*",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
